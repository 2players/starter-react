{
  "name": "starter-vanilla-html5",
  "description": "Scaffold out a common HTML5 page.",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "APP_ENV=local W_FILE_HASH=true webpack-dev-server --mode development --disable-host-check --hot",
    "build:preview": "APP_ENV=preview W_FILE_HASH=true W_FILE_FLAT=true W_MODE=production webpack --mode production",
    "preview": "npm run build:preview && echo 'placeholder'",
    "build:release": "APP_ENV=release W_FILE_HASH=true W_FILE_FLAT=true W_MODE=production WCP_PUBLIC_PATH='' webpack --mode production",
    "release": "yarn build:release && echo 'placeholder'",
    "lint": "eslint 'src/**/*.js'"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "core-js": "^3.1.4",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.2",
    "scriptjs": "^2.5.9",
    "webpack-config-parts": "^6.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{json,md}": [
        "prettier --write",
        "git add"
      ],
      "**/*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  }
}
